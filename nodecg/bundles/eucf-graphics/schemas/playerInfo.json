{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"username": {
			"type": "string"
		},
		"role": {
			"type": "string"
		},
		"age": {
			"description": "Age in years",
			"type": "integer",
			"minimum": 0
		},
		"major": {
			"type": "string"
		}
	},
	"additionalProperties": false,
	"required": ["username"],
	"default": {
		"username": "example player"
	}
}
